{
  "manifest_version": 2,
  "name": "Redirect YouTube Homepage to Google",
  "version": "1.0",
  "description": "Redirects https://www.youtube.com to Google (https://www.google.com).",
  "permissions": ["webNavigation", "webNavigation.onErrorOccurred"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}